func int my_gcd(int a, int b)
{
        while (a != b) {
                if (a > b) {
                        a = a - b;
                } else {
                        b = b - a;
                }
        }
        return a + 1;
} with test {
	assert(my_gcd(a,b) == 3);
	assert(my_gcd(78,9) == 3);
} using {
	int a;
	int b;
	a = 15;
	b = 9;
}

func int my_sum(struct int_list *l)
{
	int len;
	int sum;
	int i;
	sum = 0;
	len = int_list_length(l);
	for (i = 0; i < len; i = i + 1) {
		sum = sum + int_list_get(l,i);
	}
	
	return sum;
