	{
		name = s;
	}

};

struct car {
	int year;
	int current_year;
	string model;
	int miles;

	method void set_curr_year(int a)
	{
		current_year = a;
	}

	method int miles_per_year()
	{
		int tmp;
		tmp = current_year - year;
		tmp = miles / tmp;
		return tmp;	
	}

	method void set_model(string s)
	{
		model = s;
	}

	method void set_year(int a)
	{
		year = a;
	}

	method void set_miles(int a)
	{
		miles = a;
	}
};	
